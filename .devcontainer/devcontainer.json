// Reference: https://code.visualstudio.com/docs/remote/containers#_devcontainerjson-reference
{
  "name": "duard Angular Development Container",
  "dockerFile": "Dockerfile",
  "appPort" : ["4200:4200", "7777:7777", "4211:4211"],
  "postCreateCommand": "npm install; npm audit fix; npm audit fix --force;",
  "extensions": [
    "PKief.material-icon-theme",
    "devondcarew.bazel-code",
    "gkalpak.aio-docs-utils",
    "ms-vscode.vscode-typescript-tslint-plugin",
    "xaver.clang-format",
    "EditorConfig.EditorConfig",
    "Angular.ng-template",
    "johnpapa.angular2",
    "mikael.angular-beastcode",
    "ms-vscode.vscode-typescript-tslint-plugin",
    "eg2.vscode-npm-script",
    "christian-kohler.path-intellisense",
    "esbenp.prettier-vscode",
    "github.vscode-pull-request-github",
    "eamodio.gitlens",
    "msjsdiag.debugger-for-chrome",
    "ms-azuretools.vscode-docker",
    "shanoor.vscode-nginx"
  ],
}
